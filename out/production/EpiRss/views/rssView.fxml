<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="epirss.controller.RssController">
   <children>
      <Pane prefHeight="600.0" prefWidth="1024.0">
         <children>
            <TextField fx:id="email" layoutX="22.0" layoutY="130.0" prefHeight="27.0" prefWidth="190.0" promptText="Email" />
            <Button fx:id="signUp" layoutX="22.0" layoutY="228.0" mnemonicParsing="false" text="Sign Up" />
            <PasswordField fx:id="password" layoutX="22.0" layoutY="158.0" prefHeight="27.0" prefWidth="190.0" promptText="Password" />
            <Button fx:id="signIn" layoutX="149.0" layoutY="228.0" mnemonicParsing="false" text="Sign In" />
            <ImageView fitHeight="123.0" fitWidth="237.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
